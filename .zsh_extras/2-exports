# hack to shut homebrew up
export PATH=/Users/marekbi/Projects/homebrew/bin:/usr/bin:./bin:./script:$PATH

[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

# Use Clang as default compiler
export CC=clang
export CFLAGS=-Qunused-arguments
export CPPFLAGS=-Qunused-arguments

# Set home page for Lynx
export WWW_HOME=http://www.google.com/

# Add Google depot tools
# export PATH=$PATH:Z/git/external/depot_tools

# Fix compiler library for homebrew
export LDFLAGS="$LDFLAGS -L/Users/marekbi/Projects/homebrew/lib"

export LSCOLORS=gxfxbEaEBxxEhEhBaDaCaD

export HTTP_PROXY=http://marekbi:En!xpolik9@eurprx01:8080
